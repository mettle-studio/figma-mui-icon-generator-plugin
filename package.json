{
  "name": "figma-mui-icon-generator-plugin",
  "version": "0.0.1",
  "dependencies": {
    "@create-figma-plugin/ui": "^3.2.1",
    "@create-figma-plugin/utilities": "^3.2.1",
    "html-entities": "^2.5.2",
    "plugma": "^1.2.8",
    "preact": ">=10",
    "svgo": "^3.3.2"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^3.2.1",
    "@create-figma-plugin/tsconfig": "^3.2.1",
    "@figma/plugin-typings": "1.100.2",
    "@types/node": "^22.13.10",
    "jsdom": "^24.0.0",
    "typescript": ">=4",
    "vitest": "^1.2.2"
  },
  "scripts": {
    "dev": "plugma dev",
    "build": "plugma build",
    "preview": "plugma preview",
    "release": "plugma release",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "plugma": {
    "manifest": {
      "api": "1.0.0",
      "editorType": [
        "figma"
      ],
      "id": "mui-icon-generator",
      "name": "MUI Icon Generator",
      "main": "src/main.ts",
      "networkAccess": {
        "allowedDomains": [
          "none"
        ],
        "devAllowedDomains": [
          "*"
        ]
      }
    }
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "plugma"
    ]
  }
}
